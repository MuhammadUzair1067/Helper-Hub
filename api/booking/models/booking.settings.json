{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "name": "booking",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "duration": {
      "type": "integer"
    },
    "customer": {
      "via": "bookings",
      "model": "customer"
    },
    "cleaner": {
      "via": "bookings",
      "model": "cleaner"
    },
    "pictures": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "instructions": {
      "type": "string"
    },
    "pets": {
      "type": "boolean"
    },
    "address": {
      "type": "string"
    },
    "amount": {
      "type": "float",
      "default": 0
    },
    "date": {
      "type": "date"
    },
    "time": {
      "type": "time"
    },
    "paidBy": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "COMPLETED",
        "ACTIVE",
        "PAID",
        "UN_PAID"
      ],
      "default": "ACTIVE"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "CHEF",
        "DRIVER",
        "CLEANER",
        "MAID",
        "CHILD_CARE",
        "NURSE",
        "GARDENER",
        "ELDERCARE"
      ]
    },
    "rating": {
      "type": "integer",
      "default": 0
    },
    "hour": {
      "type": "integer",
      "default": 1
    }
  }
}
