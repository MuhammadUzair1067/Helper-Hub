{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "name": "booking",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "duration": {
      "type": "integer"
    },
    "customer": {
      "model": "customer",
      "via": "bookings"
    },
    "cleaner": {
      "via": "bookings",
      "model": "cleaner"
    },
    "bedroomCount": {
      "type": "integer",
      "default": 0
    },
    "bathroomCount": {
      "type": "integer",
      "default": 0
    },
    "pictures": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "instructions": {
      "type": "string"
    },
    "pets": {
      "type": "boolean"
    },
    "address": {
      "type": "string"
    },
    "amount": {
      "type": "integer",
      "default": 0
    },
    "date": {
      "type": "date"
    },
    "time": {
      "type": "time"
    },
    "paidBy": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "COMPLETED",
        "ACTIVE",
        "PAID",
        "UN_PAID"
      ],
      "default": "ACTIVE"
    },
    "kitchenCount": {
      "default": 1,
      "type": "integer"
    }
  }
}
